/** Add css rules here for your application. */


/** Example rules used by the template application (remove for your app) */
h1 {
  font-size: 6em;
  font-weight: bold;
  color: #77777;
  margin: 40px 0px 70px;
  text-align: center;
}

.sendButton {
  display: block;
  font-size: 16pt;
}

/** Most GWT widgets already have a style name defined */
.gwt-DialogBox {
  width: 400px;
}

.dialogVPanel {
  margin: 5px;
}

.serverResponseLabelError {
  color: red;
}

/** Set ids using widget.getElement().setId("idOfElement") */
#closeButton {
  margin: 15px 6px 6px;
}
.gwt-PageTitle {
	font-family: Helvetica, sans-serif;
	font-size: xx-large;
	font-weight: lighter;
}
.gwt-err {
	color:red
}
.gwt-link {
	color:#0000FF;
	text-decoration:underline;
}
.gwt-SubHeading {
	font-family: Helvetica, sans-serif;
	font-size: x-large;
	font-weight: lighter;
}
.gwt-smallSubHeading {
	font-family: Helvetica, sans-serif;
	font-size: large;
	font-weight: lighter;
}
.gwt-HyperlinkLeft {
	text-align:left;
}
